<Window x:Class="ChampionProfiler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Champion Profiler" Height="600" Width="425" ResizeMode="CanMinimize" Background="#FF353535">
    <Grid x:Name="ChampionSelect">
        <Image Margin="32,26,208,486" Source="title.png" Stretch="Fill"/>
        <Label Content="Select Champion" HorizontalAlignment="Left" Margin="73,147,0,0" VerticalAlignment="Top" Foreground="#FFCCCCCC"/>
        <RadioButton Content="All" HorizontalAlignment="Left" Margin="266,152,0,0" VerticalAlignment="Top" Foreground="#FFCCCCCC" IsChecked="True"/>
        <RadioButton Content="Revised" HorizontalAlignment="Left" Margin="314,152,0,0" VerticalAlignment="Top" Foreground="#FFCCCCCC"/>
        <Button Content="" HorizontalAlignment="Left" Margin="5,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Ahri" Click="load_Champion">
            <Button.Background>
                <ImageBrush ImageSource="AhriSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="60,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Akali">
            <Button.Background>
                <ImageBrush ImageSource="AkaliSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="115,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Alistar">
            <Button.Background>
                <ImageBrush ImageSource="AlistarSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="170,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Amumu">
            <Button.Background>
                <ImageBrush ImageSource="AmumuSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="225,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Anivia">
            <Button.Background>
                <ImageBrush ImageSource="AniviaSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="280,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Annie">
            <Button.Background>
                <ImageBrush ImageSource="AnnieSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="335,174,0,0" VerticalAlignment="Top" Width="50" Height="50" ToolTip="Ashe">
            <Button.Background>
                <ImageBrush ImageSource="AsheSquare.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
    </Grid>
</Window>
